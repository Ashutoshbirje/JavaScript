// Heap memory

let obj1 = {
    fname: "ashu",
    lname: "birje"
}

let obj2 = obj1

console.log(obj1);
console.log(obj2);

obj2.fname = "ashutosh"

console.log(obj1); // change 
console.log(obj2); // change 